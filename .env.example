# Copy this file to .env and fill in the values

APP_ENV=dev
APP_SECRET=change_this_to_random_secret_key

###> doctrine/doctrine-bundle ###
DATABASE_URL="postgresql://app:!ChangeMe!@db:5432/app?serverVersion=15&charset=utf8"
###< doctrine/doctrine-bundle ###

###> Telegram ###
# Get your token from @BotFather in Telegram
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
###< Telegram ###

###> DeepSeek API ###
# Get your API key from https://platform.deepseek.com/api_keys
# Optional - can use GigaChat instead
DEEPSEEK_API_KEY=your_deepseek_api_key_here
###< DeepSeek API ###

###> GigaChat API ###
# Get credentials from https://developers.sber.ru/portal/products/gigachat
# Format: CLIENT_ID:CLIENT_SECRET (from Authorization Basic header)
GIGACHAT_CLIENT_ID=your_gigachat_client_id_here
GIGACHAT_CLIENT_SECRET=your_gigachat_client_secret_here
###< GigaChat API ###

###> Admin ###
ADMIN_PASSWORD=admin123
###< Admin ###

###> symfony/messenger ###
MESSENGER_TRANSPORT_DSN=doctrine://default?auto_setup=0
###< symfony/messenger ###
